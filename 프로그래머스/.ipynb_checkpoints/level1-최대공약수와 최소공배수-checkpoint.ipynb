    "# 나의 코드\n",
    "def lcm_gcd(n, m):\n",
    "    big_num = max(n, m)\n",
    "    small_num = min(n, m)\n",
    "    lcm = max([i+1 for i in range(small_num) if small_num%(i+1) == 0 and big_num%(i+1) == 0])\n",
    "    gcd = (big_num*small_num)/lcm\n",
    "    return [lcm, gcd]\n",
    "\n",
    "print(lcm_gcd(3, 12))\n",
    "print(lcm_gcd(12, 15))"
   ]
  }
